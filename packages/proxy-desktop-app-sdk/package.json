{
  "name": "@redonvn/proxy-desktop-app-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK cho Proxy Desktop App",
  "keywords": [
    "proxy",
    "desktop",
    "cliproxy",
    "websocket",
    "sdk"
  ],
  "license": "MIT",
  "author": "Redon",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "tsup --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "ws": "^8.18.0"
  }
}

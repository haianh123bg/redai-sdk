{
  "name": "@redonvn/sepay-hub-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK cho SePay Bank Hub API v1",
  "keywords": [
    "sepay",
    "bank-hub",
    "payment",
    "sdk",
    "project03"
  ],
  "license": "MIT",
